
public class Solution{  
    
    public static int duplicateNumber(int arr[]) {
    

    int n = arr.length;
    
    for(int i =0; i<n; i++)
    {
        int duplicate = arr[i];
        for(int j =i+1; j<n; j++)
        {
            if(duplicate==arr[j])
            {
                return duplicate;
            }
        }
    }
    
    return 0;
}
    }
